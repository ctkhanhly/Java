
public class Max {

	public int max( int[] a){
		//boolean go = false;
		int max= 0;
		for (int i =0; i< a.length; i++){
			if( i ==0){
				max = a[i];
			}
			
			if(a[i+1]> max){
				//go = true;
				max = a[i+1];
			}
		}
		return max;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a = { 4,10,12,15,100,33};

}
}
