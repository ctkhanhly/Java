
public class Max {

	public static int max( int[] a){
		//boolean go = false;
		int max= 0;
		for (int i =0; i< a.length-1; i++){
			if( i ==0){
				max = a[i];
			}
			
			if(a[i+1]> max && a[i+1]%10==0){
				//go = true;
				max = a[i+1];
			}
		}
		if(max<10){
			max = 0;
		}
		return max;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a = { 4,10,12,15,100,33};
		System.out.println(max(a));

}
}
